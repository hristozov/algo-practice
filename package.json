{
  "name": "algo-practice",
  "version": "0.0.1",
  "description": "Algorithm implementations",
  "main": "index.js",
  "repository": "https://github.com/hristozov/algo-practice",
  "author": "Georgi Hristozov <georgi@forkbomb.nl>",
  "license": "Unlicense",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^9.0.0",
    "chai": "^4.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "lint": "tslint -p . -c tslint.json",
    "test": "nyc mocha -r ts-node/register \"src/**/*.spec.ts\""
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {}
}
