{
  "name": "algo-practice",
  "version": "0.0.1",
  "description": "Algorithm implementations",
  "main": "index.js",
  "repository": "https://github.com/hristozov/algo-practice",
  "author": "Georgi Hristozov <georgi@forkbomb.nl>",
  "license": "Unlicense",
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.20",
    "chai": "4.2.0",
    "mocha": "8.0.1",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "source-map-support": "0.5.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.9.6"
  },
  "scripts": {
    "lint": "tslint -p . -c tslint.json",
    "test": "nyc mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "prettier": "prettier -c ."
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {}
}
