{
  "name": "algo-practice",
  "version": "0.0.1",
  "description": "Algorithm implementations",
  "main": "index.js",
  "repository": "https://github.com/hristozov/algo-practice",
  "author": "Georgi Hristozov <georgi@forkbomb.nl>",
  "license": "Unlicense",
  "devDependencies": {
    "@types/chai": "4.2.12",
    "@types/mocha": "8.0.3",
    "@types/node": "14.6.0",
    "chai": "4.2.0",
    "mocha": "8.1.1",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "source-map-support": "0.5.19",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  },
  "scripts": {
    "lint": "tslint -p . -c tslint.json",
    "lint-fix": "tslint -p . -c tslint.json --fix",
    "test": "nyc mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "prettier": "prettier -c .",
    "prettier-fix": "prettier --write ."
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {}
}
